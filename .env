# Environment defaults for local Docker runs.
PUBLIC_BASE_URL=https://reefmetrics.app
LOG_LEVEL=INFO

# Sentry configuration (set SENTRY_DSN to enable).
SENTRY_DSN=
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_SEND_DEFAULT_PII=false
SENTRY_ENABLE_LOGS=true
SENTRY_PROFILE_SESSION_SAMPLE_RATE=0.0
SENTRY_PROFILE_LIFECYCLE=
